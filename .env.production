# Environment Configuration for Production
NODE_ENV=production

# Database (use Neon PostgreSQL for production)
# DATABASE_URL=your_neon_database_url_here

# FastText Model (disabled in Netlify due to file size)
ENABLE_FASTTEXT=false

# Session Configuration
SESSION_SECRET=your_production_session_secret_here

# CORS Configuration
CORS_ORIGIN=https://your-netlify-domain.netlify.app

# WebSocket Configuration (disabled in serverless)
ENABLE_WEBSOCKET=false
